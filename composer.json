{
  "name": "andres-montanez/magallanes",
  "description": "The Deployment Tool for PHP Applications",
  "homepage": "https://magephp.com",
  "license": "MIT",
  "type": "library",
  "keywords": ["deployment"],
  "authors": [
    {
      "name": "Andrés Montañez",
      "email": "andresmontanez@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "monolog/monolog": "^2.5|^3.0",
    "symfony/console": "^6.0",
    "symfony/filesystem": "^6.0",
    "symfony/event-dispatcher": "^6.0",
    "symfony/finder": "^6.0",
    "symfony/yaml": "^6.0",
    "symfony/process": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.5",
    "squizlabs/php_codesniffer": "^3.6",
    "php-coveralls/php-coveralls": "^2.5"
  },
  "suggest": {
    "ext-posix": "*"
  },
  "autoload": {
    "psr-4": {
      "Mage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mage\\Tests\\": "tests/"
    }
  },
  "bin": ["bin/mage"],
  "extra": {
    "branch-alias": {
      "dev-master": "5.0.x-dev",
      "dev-galactica": "5.x-dev"
    }
  }
}
