{
    "name": "andres-montanez/magallanes",
    "description": "A Deployment Tool for PHP Applications",
    "homepage": "http://magephp.com",
    "license": "MIT",
    "type": "library",
    "keywords": ["deployment"],
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.5"
    },
    "autoload": {
        "psr-4": {
            "Mage\\": "./Mage",
            "Task\\": ".mage/tasks",
            "Command\\": ".mage/commands"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "bin/mage"
    ]
}
